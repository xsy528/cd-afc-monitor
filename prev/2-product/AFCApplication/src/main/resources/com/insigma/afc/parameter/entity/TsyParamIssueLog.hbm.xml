<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.insigma.afc.parameter.entity.TsyParamIssueLog" table="TSY_PARAM_ISSUE_LOG">
    <id column="LOG_ID" length="20" name="id" type="java.lang.Long">
      <generator class="sequence">
        <param name="sequence">S_TSY_PARAM_ISSUE_LOG</param>
      </generator>
    </id>
    <property column="LINE_ID" length="5" name="lineId" type="java.lang.Short"/>
    <property column="ISSUE_TIME" length="11" name="issueTime" type="java.util.Date"/>
    <property column="OPERATOR_ID" length="20" name="operatorId" type="java.lang.String"/>
    <property column="VERSION_NUMBER" length="20" name="versionNumber" type="java.lang.Long"/>
    <property column="CURRENT_VERSION_NUMBER" length="20" name="currentVersionNumber" type="java.lang.Long"/>
    <property column="FUTURE_VERSION_NUMBER" length="20" name="futureVersionNumber" type="java.lang.Long"/>
    <property column="VERSION_PARAM_TYPE" length="5" name="versionParamType" type="java.lang.Short"/>
    <property column="ISSUE_TYPE" length="5" name="issueType" not-null="true" type="java.lang.Short"/>
    
    <set name="tsyDatas" lazy="false" cascade="all" table="TSY_PARAM_ISSUE_DATA">
    	<key column="ISSUE_LOG_ID"></key>
    	<one-to-many class="com.insigma.afc.parameter.entity.TsyParamIssueData"/>
    </set>
    
  </class>
</hibernate-mapping>
